                "event_id",
                "name",
                "date_time",
                "venue",
                "city",
                "state",
                "zip",
                "status",
                "min_price",
                "max_price"
              ],
              "properties": {
                "event_id": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "date_time": {
                  "type": "string"
                },
                "venue": {
                  "type": "string"
                },
                "city": {
                  "type": "string"
                },
                "state": {
                  "type": "string"
                },
                "zip": {
                  "type": "string"
                },
                "status": {
